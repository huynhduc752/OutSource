    <script src="~/Content/js/zoom/js/setup.js"></script>
    <script>

        $(document).ready(function () {
            $(window).scroll(function (event) {
                if ($(this).scrollTop() > 85) {
                    $('.header').addClass("fixed-top");
                    $('.header li.link-GAQ ').removeClass("d-none");
                } else {
                    $('.header ').removeClass("fixed-top");
                    $('.header li.link-GAQ ').addClass("d-none");

                }
            });

        });
        $('.owl-carousel').owlCarousel({
            loop: true,
            margin: 10,
            autoplay: false,
            slidesToShow: 4,
            autoplayTimeout: 1500,
            autoplayHoverPause: false,
            nav: true,
            responsive: {
                0: {
                    items: 3
                },
                992: {
                    items: 4
                }
            }
        });
        $('.regular-intro').slick({
            loop: true,
            dots: false,
            infinite: true,
            autoplay: true,
            autoplayTimeout: 1500,
            slidesToShow: 1,
            nextArrow: "<img class='a-left control-c prev slick-next' src='../Content/images/home/right.png'>",
            prevArrow: "<img class='a-left control-c prev slick-prev' src='../Content/images/home/left.png'>",
            slidesToScroll: 1,

        })
        $('.regular-two-option').slick({
            loop: true,
            dots: false,
            infinite: true,
            autoplay: true,
            autoplayTimeout: 1500,
            slidesToShow: 2,
            nextArrow: "<img class='a-left control-c prev slick-next' src='../Content/images/home/right.png'>",
            prevArrow: "<img class='a-left control-c prev slick-prev' src='../Content/images/home/left.png'>",
            slidesToScroll: 1,
            responsive: {
                0: {
                    items: 1
                },
                768: {
                    items: 1
                },
                1200: {
                    items: 2
                }
            }

        })
        $('.regular-edit').slick({
            loop: true,
            dots: false,
            infinite: true,
            autoplay: true,
            autoplayTimeout: 1500,
            slidesToShow: 4,
            nextArrow: "<img class='a-left control-c prev slick-next' src='../Content/images/home/right.png'>",
            prevArrow: "<img class='a-left control-c prev slick-prev' src='../Content/images/home/left.png'>",
            slidesToScroll: 1,
            responsive: {
                0: {
                    items: 1
                },
                600: {
                    items: 2
                },
                1000: {
                    items: 3
                },
                1500: {
                    items: 4
                }
            }

        })
        $('.regular-title-item').slick({
            loop: true,
            dots: false,
            infinite: true,
            autoplay: true,
            autoplayTimeout: 1500,
            nextArrow: "<img class='a-left control-c prev slick-next' src='../Content/images/home/right.png'>",
            prevArrow: "<img class='a-left control-c prev slick-prev' src='../Content/images/home/left.png'>",
            slidesToScroll: 1,
            responsive: [
                {
                    breakpoint: 2000,
                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 993,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 2
                    }
                }
            ]
        })
    </script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
